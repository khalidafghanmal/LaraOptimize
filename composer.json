{
    "name": "khalidafghanmal/response-optimizer",
    "description": "Advanced Laravel package to auto-optimize HTML/JSON responses",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Khalid\\ResponseOptimizer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Khalid\\ResponseOptimizer\\ResponseOptimizerServiceProvider"
            ],
            "aliases": {
                "ResponseOptimizer": "Khalid\\ResponseOptimizer\\Facades\\ResponseOptimizer"
            }
        }
    },
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
